{
  "name": "oyster-package-generator",
  "version": "0.0.0-development",
  "description": "CLI to automatically generate a Unity Package Manager project",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && jest",
    "test:watch": "npm run lint && jest --watch",
    "build": "npm run lint && tsc",
    "start": "npm run build && node dist/index.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "bin": {
    "oyster": "./bin/oyster"
  },
  "author": "Ash Blue",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/del": "^4.0.0",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^24.8.0",
    "semantic-release": "^15.13.21",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "copy-dir": "^1.1.0",
    "del": "^4.1.1",
    "git-remote-origin-url": "^3.0.0",
    "glob": "^7.1.4",
    "inquirer": "^6.4.1",
    "shelljs": "^0.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashblue/oyster-package-generator.git"
  },
  "keywords": [
    "unity",
    "unity3d",
    "upm",
    "package generator",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/ashblue/oyster-package-generator/issues"
  },
  "homepage": "https://github.com/ashblue/oyster-package-generator#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
